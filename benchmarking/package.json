{
  "name": "benchmarking",
  "version": "0.0.1",
  "description": "Wrappers to benchmark LumoSQL",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LumoSQL/LumoSQL.git"
  },
  "keywords": [
    "LumoSQL",
    "benchmarking",
    "SQLite",
    "SQLightning"
  ],
  "author": "Keith Maxwell",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/LumoSQL/LumoSQL/issues"
  },
  "homepage": "https://github.com/LumoSQL/LumoSQL#readme",
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "prettier"
    ],
    "rules": {
      "sort-imports": [
        "error",
        {
          "ignoreDeclarationSort": true
        }
      ],
      "import/order": "error"
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "plugins": [
      "svelte3"
    ],
    "overrides": [
      {
        "files": [
          "**/*.svelte"
        ],
        "processor": "svelte3/svelte3"
      }
    ]
  },
  "eslintIgnore": [
    "*.html",
    "*.json",
    "*.png",
    "/out/",
    "/src/node_modules/"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.1",
    "polka": "^0.5.2",
    "sirv": "^1.0.0-next.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.8.7",
    "@babel/runtime": "^7.8.4",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.2.0",
    "ajv": "^6.12.0",
    "c8": "^7.0.0",
    "cypress": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "js-yaml": "^3.13.1",
    "jsdoc": "^3.6.3",
    "mocha": "^7.1.1",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.0",
    "start-server-and-test": "^1.10.11",
    "svelte": "^3.20.1"
  },
  "resolutions": {
    "cypress/extract-zip/mkdirp": "^0.5.3",
    "cypress/minimist": "^1.2.5"
  },
  "scripts": {
    "apidocs": "jsdoc -c .jsdoc.conf.json",
    "check-coverage": "c8 check-coverage --functions 100 --lines 100 --branches 100 --statements 100 --per-file",
    "format": "prettier --write '*.{js,json}' '**/*.{js,json,mjs,svelte}'",
    "gh-pages": "sapper export && rm -rf gh-pages/* && cp -r __sapper__/export/* gh-pages/ && git -C gh-pages add .",
    "lint": "eslint 'src/**' 'cypress/**' 'test/**'",
    "test": "yarn run unit && cypress run",
    "unit": "mocha --experimental-modules"
  }
}
